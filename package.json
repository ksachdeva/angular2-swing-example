{
  "name": "angular2-swing-example",
  "version": "1.0.0",
  "description": "Angular 2 Swing example",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run typings install",
    "build": "tsc",
    "build:w": "tsc --watch",
    "bundle": "node bundle.js",
    "bundle:prod": "node bundle.js --prod",
    "lite": "lite-server",
    "start": "concurrently \"npm run build:w\" \"npm run lite\" ",
    "typings": "typings"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "angular2-swing": "^0.5.0",
    "es6-shim": "^0.35.1",
    "hammerjs": "^2.0.8",
    "lodash": "^4.13.1",
    "raf": "^3.2.0",
    "rebound": "0.0.13",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "sister": "^3.0.0",
    "swing": "^3.0.3",
    "systemjs": "0.19.27",
    "vendor-prefix": "^0.1.0",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "lite-server": "^2.2.0",
    "typescript": "1.8.10",
    "typings": "^1.0.4",
    "systemjs-builder": "^0.15.23",
    "yargs": "^4.7.1"
  },
  "author": "Kapil Sachdeva",
  "license": "MIT"
}
